{
  "name": "vscode-webview-elements",
  "version": "1.0.0",
  "description": "",
  "main": "dist/vscodeWebviewElements.js",
  "scripts": {
    "clean": "npx rimraf dist && npx rimraf storybook-static",
    "start": "concurrently \"rollup --config --watch\" \"serve\" --names \"rollup,serve\" -k",
    "build": "rollup --config",
    "build-all": "npm run clean && npm run svgo && npm run build && npm run build-storybook",
    "storybook": "start-storybook -p 6006 -s ./dist,./assets",
    "build-storybook": "build-storybook -s ./dist,./assets",
    "svgo": "svgo -f vscode-icons/icons/ -r --config=.svgo.yml -p 5 -o dist/icons",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/html": "^5.2.1",
    "@types/jest": "^24.0.18",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.0",
    "jest": "^24.9.0",
    "jest-puppeteer": "^4.3.0",
    "puppeteer": "^1.20.0",
    "rollup": "^1.23.1",
    "rollup-plugin-minify-html-literals": "^1.2.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript": "^1.0.1",
    "serve": "^11.2.0",
    "svgo": "^1.3.0",
    "ts-jest": "^24.1.0",
    "tslib": "^1.10.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "lit-element": "^2.2.1"
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "staticFiles": {
    "staticPath": [
      {
        "outDirPattern": "**/dist",
        "staticPath": "vscode-icons-optimized"
      }
    ]
  }
}
