{
  "name": "vscode-webview-elements",
  "version": "1.0.0",
  "description": "",
  "main": "dist/vscodeWebviewElements.js",
  "scripts": {
    "clean": "npx rimraf dist && npx rimraf storybook-static && npx rimraf vscode-icons-optimized",
    "dev": "parcel sandbox.html",
    "build": "parcel build components/vscodeWebviewElements.ts",
    "build-all": "npm run clean && npm run svgo && npm run build && npm run build-storybook",
    "storybook": "start-storybook -p 6006 -s ./dist,./assets",
    "build-storybook": "build-storybook -s ./dist,./assets",
    "svgo": "svgo -f vscode-icons/icons/ -r --config=.svgo.yml -p 5 -o vscode-icons-optimized/icons"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/html": "^5.2.1",
    "babel-loader": "^8.0.6",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "svgo": "^1.3.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "lit-element": "^2.2.1"
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "staticFiles": {
    "staticPath": [
      {
        "outDirPattern": "**/dist",
        "staticPath": "vscode-icons-optimized"
      }
    ]
  }
}
